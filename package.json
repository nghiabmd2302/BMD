{
  "name": "@tsed/typeorm-example",
  "version": "1.0.0",
  "description": "Here an example project with TypeORM and Ts.ED framework.",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "yarn tsc",
    "test": "yarn clean && yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start:dev": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node --files src/index.ts",
    "start": "ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "yarn build && docker-compose build",
    "deploy": "exit 0"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tsed/ajv": "^7.16.2",
    "@tsed/common": "^5.67.2",
    "@tsed/core": "5.67.2",
    "@tsed/di": "5.67.2",
    "@tsed/exceptions": "5.67.2",
    "@tsed/multipartfiles": "^5.67.2",
    "@tsed/passport": "5.65.3",
    "@tsed/platform-express": "5.67.2",
    "@tsed/platform-middlewares": "^7.16.0",
    "@tsed/platform-params": "^7.16.0",
    "@tsed/schema": "^5.67.2",
    "@tsed/swagger": "5.65.3",
    "@tsed/testing": "5.67.2",
    "@tsed/typeorm": "5.65.3",
    "@types/swagger-schema-official": "2.0.21",
    "bcryptjs": "^2.4.3",
    "body-parser": "1.19.0",
    "catch-decorator-ts": "^0.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compression": "1.7.4",
    "cookie-parser": "1.4.5",
    "cors": "^2.8.5",
    "cross-env": "7.0.2",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "1.17.1",
    "express-validator": "^6.15.0",
    "i": "^0.3.7",
    "joi": "^17.8.3",
    "joi-class-decorators": "^1.1.0",
    "jsonwebtoken": "^9.0.0",
    "method-override": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "npm": "^9.6.0",
    "passport": "0.4.1",
    "passport-http": "0.3.0",
    "passport-local": "1.0.0",
    "typeorm": "^0.3.12"
  },
  "devDependencies": {
    "@types/cors": "2.8.6",
    "@types/express": "^4.17.17",
    "@types/http-proxy": "^1.16.2",
    "@types/node": "14.0.27",
    "@types/passport": "1.0.4",
    "@types/passport-http": "0.3.8",
    "@types/passport-local": "^1.0.33",
    "nodemon": "1.19.1",
    "ts-node": "9.0.0",
    "typescript": "3.9.4"
  }
}
